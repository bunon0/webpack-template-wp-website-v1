{
	"name": "webpack-template-html-website-v2",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"scripts": {
		"dev": "webpack --mode development --devtool=source-map",
		"build": "webpack --mode production",
		"start": "webpack --mode development --watch --devtool=source-map",
		"watch": "webpack --mode development --watch",
		"lint": "run-p --continue-on-error lint:*",
		"lint:eslint": "eslint --ext .js",
		"lint:prettier": "prettier --check **/*.{js,md,html,css,scss,json}",
		"lint:stylelint": "stylelint src/**/_*.scss src/**/*.{css,scss}",
		"fix": "run-s --continue-on-error fix:eslint fix:prettier",
		"fix:eslint": "eslint --ext .js . --fix",
		"fix:prettier": "prettier --write **/*.{js,md,html,css,scss,json}",
		"fix:stylelint": "stylelint src/**/_*.scss src/**/*.scss --fix"
	},
	"devDependencies": {
		"@babel/core": "^7.16.12",
		"@babel/preset-env": "^7.16.11",
		"@prettier/plugin-php": "^0.18.1",
		"@squoosh/lib": "^0.4.0",
		"autoprefixer": "^10.4.2",
		"babel-loader": "^8.2.3",
		"browser-sync": "^2.27.7",
		"browser-sync-webpack-plugin": "^2.3.0",
		"clean-webpack-plugin": "^4.0.0",
		"copy-webpack-plugin": "^10.2.4",
		"css-declaration-sorter": "^6.1.4",
		"css-loader": "^6.5.1",
		"css-minimizer-webpack-plugin": "^3.4.1",
		"css-mqpacker": "^7.0.0",
		"eslint": "^8.8.0",
		"eslint-config-prettier": "^8.3.0",
		"file-loader": "^6.2.0",
		"glob-import-loader": "^1.2.0",
		"image-minimizer-webpack-plugin": "^3.2.3",
		"mini-css-extract-plugin": "^2.5.3",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.5",
		"postcss-combine-duplicated-selectors": "^10.0.3",
		"postcss-loader": "^6.2.1",
		"prettier": "^2.5.1",
		"sass": "^1.49.0",
		"sass-loader": "^12.4.0",
		"style-loader": "^3.3.1",
		"stylelint": "^14.3.0",
		"stylelint-config-prettier": "^9.0.3",
		"stylelint-config-standard": "^24.0.0",
		"stylelint-config-standard-scss": "^3.0.0",
		"stylelint-selector-bem-pattern": "^2.1.1",
		"terser-webpack-plugin": "^5.3.0",
		"webpack": "^5.68.0",
		"webpack-cli": "^4.9.2"
	},
	"dependencies": {}
